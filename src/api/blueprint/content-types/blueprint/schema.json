{
  "kind": "collectionType",
  "collectionName": "blueprints",
  "info": {
    "singularName": "blueprint",
    "pluralName": "blueprints",
    "displayName": "Blueprint",
    "description": "Factory blueprints for Arknights: Endfield"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Slug": {
      "type": "uid",
      "targetField": "Title"
    },
    "Description": {
      "type": "richtext",
      "required": true
    },
    "DetailDescription": {
      "type": "richtext"
    },
    "ImportString": {
      "type": "text",
      "required": true
    },
    "ImportCodes": {
      "type": "json"
    },
    "Upvotes": {
      "type": "integer",
      "default": 0
    },
    "Screenshot": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": ["images"]
    },
    "PreviewImage": {
      "type": "string"
    },
    "ProductIcon": {
      "type": "string"
    },
    "ProductName": {
      "type": "string"
    },
    "Region": {
      "type": "enumeration",
      "enum": ["Asia", "NA / EU", "CN"],
      "required": false
    },
    "Author": {
      "type": "string"
    },
    "Tags": {
      "type": "json"
    },
    "Operators": {
      "type": "json"
    },
    "Category": {
      "type": "enumeration",
      "enum": ["Production", "Processing", "Assembly", "Power", "Storage", "Complete Chain", "Compact", "Megafactory"],
      "default": "Production"
    },
    "Complexity": {
      "type": "enumeration",
      "enum": ["Beginner", "Intermediate", "Advanced", "Expert"],
      "default": "Beginner"
    },
    "BuildingCount": {
      "type": "integer"
    },
    "GridSize": {
      "type": "string"
    },
    "NetPower": {
      "type": "integer"
    },
    "OutputsPerMin": {
      "type": "decimal"
    },
    "OutputsData": {
      "type": "json"
    },
    "FacilityCount": {
      "type": "integer"
    },
    "Status": {
      "type": "enumeration",
      "enum": ["pending", "approved", "rejected"],
      "default": "pending"
    },
    "SubmittedAt": {
      "type": "datetime"
    },
    "Recipes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::recipe.recipe"
    }
  }
}
